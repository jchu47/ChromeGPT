{
  "name": "Bad Vibes Blocker",
  "version": "1.0.0",
  "description": "Gives a rating of the current websites vibes.",
  "manifest_version": 3,
  "author": "Moiz Rauf & Justin Chu",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://api.openai.com/*"
  ],
  "icons": {
    "128": "assets/joy.png"
  },
  "action":{
      "default_popup": "index.html",
      "default_title": "Bad Vibes Blocker"
  }
}